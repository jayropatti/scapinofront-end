{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "airbnb-base"
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
    "linebreak-style": ["error", "windows"], //zorgt voor geen linebreak errror
    "rules": {
        "no-unused-vars": 0 //zorgt voor geen melding bij ongebruikte variabelen 
    }
  }
}